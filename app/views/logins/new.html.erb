<p>
	<h1> Login </h1>
	<hr/>
</p>

<div class="row">
	<div class="well col-md-4 col-md-offset-4">
		<%= form_tag '/login' do %>

		<%= label_tag :email %>
		<%= text_field_tag :email %>

		<%= label_tag :password %>
		<%= password_field_tag :password %>

		<%= submit_tag 'Login', class: 'btn btn-success' %>

		<% end %>
	</div>
</div>
	
